# .env.example -- base your .env that is not tracked by the repo on this
# ----- GeoServer -----
GEOSERVER_IMAGE=kartoza/geoserver:2.25.1
GEOSERVER_ADMIN_USER=admin
GEOSERVER_ADMIN_PASSWORD=change_me
GEOSERVER_JAVA_OPTS=-Xms1g -Xmx2g -XX:ActiveProcessorCount=2 -Duser.timezone=UTC
GEOSERVER_LOCAL_PORT=8080

# ----- Traefik / Prod -----
LETSENCRYPT_EMAIL=you@example.com
GEOSERVER_HOST=geoserver.example.com
TRAEFIK_DASHBOARD=false
